import java.util.*;

public class GenericPrint {
    
    public static <T> void printList(List<T> list) {
        for (T element : list) {
            System.out.println(element);
        }
    }

    public static void main(String[] args) {
    
        List<Integer> intList = Arrays.asList(10, 20, 30, 40);
        System.out.println("Integer List Elements:");
        printList(intList);

        List<String> strList = Arrays.asList("Apple", "Banana", "Cherry");
        System.out.println("\nString List Elements:");
        printList(strList);
    }
}